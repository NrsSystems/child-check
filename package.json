{
  "name": "child-check",
  "version": "1.0.0",
  "description": "A basic check-in system for children",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Xsponse",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1"
  },
  "build": {
    "appId": "com.Xsponse.Childcheck",
    "files": [
      "main.js"
    ],
    "extraResources": [
      "server/"
    ],
    "mac": {
      "category": "Utility"
    },
    "linux": {
      "category": "Utility",
      "target": ["AppImage", "tar.gz"]
    }
  }
}
